<div ng-repeat="snake in snakes">
    <div class="span6">
        <div class="row">
            <div class="well">
                <fieldset>
                    <legend>Snake {{snake.snakeName}}
                        <a href="#!/Snake/Edit/{{snake.id}}" class="btn  btn-mini">Edit</a>
                        <button ng-click="delete(snake)" class="btn  btn-mini">Delete</button>
                    </legend>
                    <p>Created: <span>{{snake.created}}</span></p>
                    <p>Score: <span>{{snake.score}}</span></p>
                    <p>Matches: <span>{{snake.matches}}</span></p>
                    <p>Loses: <span>{{snake.loses}}</span></p>
                    <p>Wins: <span>{{snake.wins}}</span></p>
                </fieldset>

            </div>
        </div>
    </div>
</div>
<div class="span6">
    <div class="row">
        <a href="#!/Snake/Edit/" class="btn">Add</a>
    </div>
</div>
